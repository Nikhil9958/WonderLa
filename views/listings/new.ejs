<% layout('/layouts/boilerplate') %>
<body>
    <h3>Create new Listing</h3>
    <form method="post" action="/listings">
        <input size="50" type="text"  name="title" placeholder="enter title">
        <br><br>
        <textarea rows="5" cols="50" name="description" placeholder="enter description" id=""></textarea>
        <br><br>
        <input size="50" type="text"  name="image" placeholder="enter image URL">
        <br><br>
        <input type="number"  name="price" placeholder="enter price">
        <br><br>
        <input size="50" type="text"  name="location" placeholder="enter city/state">
        <br><br>
        <input size="50" type="text"  name="country" placeholder="enter country">
        <br><br>
       
        <button type="submit">Add</button>
    </form>
    
</body>
